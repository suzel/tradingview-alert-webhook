// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/*.js.map": true,
    "node_modules": true,
    "package-lock.json": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true
  },
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  }
}
